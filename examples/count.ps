func count_to(maxcnt) {
  i = 0
  while i < maxcnt {
    i += 1
    print(i)
  }
}

func sum(list) {
  ind = 0
  sum = 0
  while ind < list:len() {
    sum += list(ind)
    "thats right lists are indexed with ()"
    "deal with it"
    ind += 1
  }
  return sum
}

max = num(input("How high to count to? "))
